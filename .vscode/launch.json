{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Go Server",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/cmd/server",
        "env": {
          "ONNXRUNTIME_DIR": "/usr/local",
          "CGO_CFLAGS": "-I/usr/local/include/onnxruntime",
          "CGO_LDFLAGS": "-L/usr/local/lib -lonnxruntime"
        }
      }
    ]
  }
  